$url = "https://www.google.com/url?sa=i&url=https%3A%2F%2Fmoulshree.medium.com%2Fbecome-undefeatable-the-philosophy-of-miyamoto-musashi-b9e8eaebd156&psig=AOvVaw1LsZ4BNL4-PH2zwc5gSey0&ust=1695369140731000&source=images&cd=vfe&opi=89978449&ved=0CBAQjRxqFwoTCIjT8K2cu4EDFQAAAAAdAAAAABAE"
$tempPath = [System.IO.Path]::GetTempFileName() + ".jpg"
Invoke-WebRequest -Uri $url -OutFile $tempPath
$style = 0
$position = 0
$code = @"
using System;
using System.Runtime.InteropServices;

public class WallpaperChanger {
    [DllImport("user32.dll", CharSet = CharSet.Auto)]
    public static extern int SystemParametersInfo(uint uAction, uint uParam, string lpvParam, uint fuWinIni);
}
"@
Add-Type -TypeDefinition $code
[WallpaperChanger]::SystemParametersInfo(20, 0, $tempPath, 3)
